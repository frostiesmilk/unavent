{# empty Twig template #}
{% extends '::base_frontoffice.html.twig' %}

{% block stylesheets %}
    {{parent()}}
    <link rel="stylesheet" href="{{ asset('assets/css/EventBundle/generic-event.css') }}"/> 
    <link rel="stylesheet" href="{{ asset('assets/css/ProfileBundle/generic-profile.css') }}"/> 
{% endblock %}

{% block smallMenu %}
    {{parent()}}
    <!-- BEGIN small screen menu -->
    {% include 'FlowberEventBundle::partials/eventNavbarSmall.html.twig' %}
    <!-- END small screen menu -->
{% endblock %}

{% block body %}

    <!-- Main menu -->
    {% include 'FlowberFrontOfficeBundle::partials/common/main-connected-navbar.html.twig' %}
    
    <!-- Event Banner -->  
    {% include 'FlowberCircleBundle::partials/cover.html.twig' %}           

    <!-- Second menu -->
    {% include 'FlowberEventBundle::partials/eventNavbar.html.twig' %}
   
    <!-- page -->
    <div id="friends" class="user-profile-friends user-profile">
        <div class="user-profile-category-row text-center">
            <span class="user-profile-category-title">Participants</span>
        </div>
          
        <div class="container">
            <div class="row generic-profile-list">
                {% for friend in members %}
                    <div class=" col-md-4 col-sm-6 col-xs-12">
                    {% include 'FlowberProfileBundle::partials/oneProfileResume.html.twig' %}
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
   
    <!-- footer -->                  
    {% include 'FlowberFrontOfficeBundle::partials/common/footer.html.twig' %}
    
{% endblock %}

{% block javascripts %}
    {{parent()}}

{% endblock %}